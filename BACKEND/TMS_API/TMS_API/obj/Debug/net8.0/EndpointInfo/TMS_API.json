    "/api/AuthMasterController/ChangePassword": {
      "post": {
        "tags": [
          "AuthMaster"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "type": "object",
                "properties": {
                  "UserID": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "UserName": {
                    "type": "string"
                  },
                  "CurrentPassword": {
                    "type": "string"
                  },
                  "NewPassword": {
                    "minLength": 8,
                    "pattern": "^(?=.*[a-zA-Z])(?=.*\\d)(?=.*[^a-zA-Z0-9]).{8,}$",
                    "type": "string"
                  }
                }
              },
              "encoding": {
                "UserID": {
                  "style": "form"
                },
                "UserName": {
                  "style": "form"
                },
                "CurrentPassword": {
                  "style": "form"
                },
                "NewPassword": {
                  "style": "form"
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "OK"
          }
        }
      }
    },
